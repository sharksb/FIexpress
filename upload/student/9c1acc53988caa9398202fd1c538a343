import Vue from 'vue'
import VueRouter from 'vue-router'
import Home from '../views/Home.vue'
import onlineExam from '../views/onlineExam.vue'
import wjxDemo from '../views/wjxDemo.vue'

Vue.use(VueRouter)

  const routes = [
  {
    path: '/',
    name: 'Home',
    component: Home
  },
  {
    path: '/onlineExam',
    name: 'onlineExam',
    component: onlineExam
  },
  
  {
    path: '/wjxDemo',
    name: 'wjxDemo',
    component: wjxDemo
  }
]

const router = new VueRouter({
  routes
})

export default router
